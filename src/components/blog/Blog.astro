---
import { render } from "astro:content";
const { post, isFirst = false } = Astro.props;

const { Content } = await render(post);
---

<div class="-g">
  <div class="-g_1_8 -title -without_header">
    <h1 class="-fs_l -ja-serif -font-heitai -title_label">
      {post.data.title}
    </h1>
  </div>
  <Content />
</div>
